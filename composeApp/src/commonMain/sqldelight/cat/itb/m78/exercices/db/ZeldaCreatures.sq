CREATE TABLE IF NOT EXISTS ZeldaCreatures (
    id INTEGER PRIMARY KEY NOT NULL
);

selectAll:
SELECT * FROM ZeldaCreatures;

selectById:
SELECT * FROM ZeldaCreatures WHERE id = ?;

insert:
INSERT INTO ZeldaCreatures(id) VALUES (?);

deleteAll:
DELETE FROM ZeldaCreatures;

delete:
DELETE FROM ZeldaCreatures WHERE id = ?;